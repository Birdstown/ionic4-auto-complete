    "@angular/core": "^8.0.0 || 9.0.0 || ^10.0.0",
    "@angular/forms": "^8.0.0 || 9.0.0 || ^10.0.0",
    "@angular/platform-browser": "^8.0.0 || 9.0.0 || ^10.0.0",
    "@angular/platform-browser-dynamic": "^8.0.0 || 9.0.0 || ^10.0.0",
    "@angular/router": "^8.0.0 || 9.0.0 || ^10.0.0",
