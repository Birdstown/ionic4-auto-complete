{
  "name": "ionic4-auto-complete",
  "version": "1.10.0",
  "scripts": {
    "publish": "npm publish dist",
    "build": "ng-packagr -p package.json && cp -fr src/scss/auto-complete.scss dist/auto-complete.scss",
    "docs": "npm run docs:build",
    "docs:build": "compodoc -p tsconfig.json -n ionic4-auto-complete -d docs --hideGenerator",
    "docs:serve": "npm run docs:build -- -s",
    "docs:watch": "npm run docs:build -- -s -w",
    "lint": "tslint --type-check --project tsconfig.json src/**/*.ts",
    "test": "tsc && karma start"
  },
  "ngPackage": {
    "lib": {
      "cssUrl": "inline",
      "entryFile": "src/index.ts",
      "styleIncludePaths": [
        "./src/scss"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jrquick17/ionic4-auto-complete"
  },
  "author": {
    "name": "Jeremy Quick",
    "email": "me@jrquick.com"
  },
  "keywords": [
    "angular"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jrquick17/ionic4-auto-complete/issues"
  },
  "devDependencies": {
    "@angular-devkit/core": "^7.3.9",
    "@angular-devkit/schematics": "^7.3.9",
    "@angular/common": "^7.2.16",
    "@angular/compiler": "7.2.15",
    "@angular/compiler-cli": "7.2.15",
    "@angular/core": "^7.2.16",
    "@angular/forms": "7.2.15",
    "@angular/http": "7.2.15",
    "@angular/platform-browser": "^7.2.16",
    "@angular/platform-browser-dynamic": "7.2.15",
    "@angular/router": "^7.2.16",
    "@compodoc/compodoc": "^1.1.9",
    "@ionic-native/core": "5.6.0",
    "@ionic-native/splash-screen": "5.6.0",
    "@ionic-native/status-bar": "5.6.0",
    "@ionic/angular": "^4.4.2",
    "@ionic/app-scripts": "3.2.3",
    "@ionic/cli-plugin-cordova": "1.6.2",
    "@ionic/cli-plugin-ionic-angular": "1.4.1",
    "@ionic/pro": "2.0.4",
    "@ionic/storage": "2.2.0",
    "@types/jasmine": "3.4.0",
    "@types/node": "^12.7.2",
    "codelyzer": "~5.1.0",
    "cordova-plugin-ionic": "^5.4.6",
    "eslint": "^5.16.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-cli": "~2.0.0",
    "karma-coverage-istanbul-reporter": "^2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "ng-packagr": "^5.5.0",
    "protractor": "~5.4.2",
    "rxjs": "6.5.2",
    "sw-toolbox": "3.6.0",
    "ts-node": "~8.3.0",
    "tsickle": "^0.37.0",
    "tslint": "~5.19.0",
    "typescript": "^3.2.4",
    "webpack": "^3.12.0",
    "zone.js": "^0.8.29"
  },
  "engines": {
    "node": ">=6.0.0"
  }
}
